<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="tituloApp">
        <item name="android:textSize">@dimen/titulo_app</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
        <item name="android:textColor">@color/colorAccent</item>
    </style>
    <style name="editText">
        <item name="android:textSize">@dimen/input</item>
        <item name="android:layout_marginTop">@dimen/margin</item>
        <item name="android:textColorHint">@color/Black</item>
    </style>
    <style name="button">
        <item name="android:background">@drawable/fundo_botao_1</item>
        <item name="android:state_pressed">@drawable/efeito_de_clique</item>
    </style>
    <style name="botao2" >
        <item name="android:background">@drawable/fundo_botao_2</item>
        <item name="android:state_pressed">@drawable/efeito_de_clique</item>
    </style>
</resources>